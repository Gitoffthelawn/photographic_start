{
  "manifest_version": 2,
  "name": "Photographic Start",
  "version": "1.0",

  "applications": {
    "gecko": {
      "id": "photographicstart@robacarp",
      "strict_min_version": "42.0"
    }
  },

  "description": "A start page founded on beauty.",

  "icons": {
    "48": "button/icon.svg",
    "96": "button/icon.svg"
  },

  "chrome_settings_overrides": {},
  "chrome_url_overrides": {
    "newtab": "content/new_tab.html"
  },

  "options_ui": {
    "page": "options/options.html",
    "browser_style": true
  },

  "permissions": [
    "storage"
  ]
}
